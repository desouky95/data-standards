{
  "$schema": "https://turbo.build/schema.json",

  "pipeline": {
    "build": {
      "cache": false,
      "dependsOn": ["parse-data", "build:types"]
    },
    "build:types": {
      "cache": false
    },
    "lint": {},
    "get-data": {
      "cache": false,
      "outputMode": "full",
      "inputs": ["src/**"]
    },
    "normalize-data": {
      "cache": false,
      "dependsOn": ["get-data"]
    },
    "parse-data": {
      "cache": false,
      "dependsOn": ["normalize-data"]
    }
  }
}
