{
  "$schema": "http://json.schemastore.org/swcrc",
  "module": {
    "type": "umd",
    // These are defaults.
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  },
  "isModule": true,
  "jsc": {
    "target": "es2016",
    "parser": {
      "syntax": "typescript",
      "dynamicImport": true
    }
  }
}
