{
  "name": "@desoukysvyc/currency",
  "version": "0.0.17",
  "main": "./dist/index.js",
  "scripts": {
    "build:types": "tsc ",
    "transpile": "swc ./src -d dist --copy-files --config-file ../../.swcrc",
    "build": "npm run build:types && npm run transpile",
    "copy-package": "ts-node ../../utils/copyPackage.ts",
    "prepublish": "npm run copy-package",
    "publish": "npm publish dist",
    "test" : "mocha --recursive --require ts-node/register test/*.spec.ts"
  },
  "dependencies": {
    "@desoukysvyc/data-collector": "*"
  },
  "peerDependencies": {
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "ts-node": "^10.9.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "./dist"
  }
}
