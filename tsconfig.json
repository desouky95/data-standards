{
  "compilerOptions": {
    // "target": "es2016",
    "target": "ES2020",
    "module": "ES2020",
    "lib": ["ES2020"],
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "strictPropertyInitialization": true,
    "noImplicitAny": false,
    "moduleResolution": "Bundler"
  },
  "ts-node": {
    "esm": true,
    "experimentalResolver": true,
    "files": true,
    "experimentalSpecifierResolution": "node"
  }
}
